      <attribute name="data-ng-hide" type="CDATA" defaultValueType="#IMPLIED" />
      <attribute name="data-ng-href" type="CDATA" defaultValueType="#IMPLIED" />
      <attribute name="data-ng-if" type="CDATA" defaultValueType="#IMPLIED" />
