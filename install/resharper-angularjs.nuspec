<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>AngularJS</id>
    <version>1.1.4</version>
    <authors>JetBrains</authors>
    <owners>JetBrains</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/JetBrains/resharper-angularjs</projectUrl>
    <licenseUrl>https://raw.github.com/JetBrains/resharper-angularjs/master/license.txt</licenseUrl>
    <iconUrl>https://raw.github.com/JetBrains/resharper-angularjs/master/icon.png</iconUrl>
    <description>Code completion and live templates for AngularJS</description>
    <releaseNotes>Staged code completion for AngularJS attributes
&#8226; Parsing of AngularJS expressions in attributes
&#8226; Highlighting of syntax errors in AngularJS expressions in attributes
&#8226; Support for ReSharper 8.2 EAP
        
        ReSharper 8.2 support</releaseNotes>
    <dependencies>
      <dependency id="ReSharper" version="[8.1,8.3)" />
    </dependencies>
    <tags>resharper angular angularjs javascript js typescript</tags>
  </metadata>
  <files>
    <!-- ReSharper 8.1 -->
    <file src="..\src\resharper-angularjs\bin\Release\JetBrains.ReSharper.Plugins.AngularJS.8.1.dll"
          target="ReSharper\v8.1\plugins\" />

    <!-- ReSharper 8.2 -->
    <file src="..\src\resharper-angularjs\bin\Release\JetBrains.ReSharper.Plugins.AngularJS.8.2.dll"
          target="ReSharper\v8.2\plugins\" />

    <!-- Templates -->
    <file src="..\src\resharper-angularjs\bin\Release\templates.dotSettings"
          target="ReSharper\vAny\settings\templates.dotSettings" />
  </files>
</package>
